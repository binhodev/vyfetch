{
    "name": "vyfetch",
    "version": "0.0.1",
    "description": "Consume your apis in React projects quickly and easily",
    "keywords": [
        "fetch",
        "axios",
        "react",
        "nextjs",
        "typescript",
        "http",
        "api"
    ],
    "author": "Binho.dev <silvinhofranca.dev@icloud.com>",
    "homepage": "https://github.com/binhodev/vyfetch#readme",
    "license": "ISC",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "type": "commonjs",
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup",
        "lint": "tsc",
        "test": "vitest",
        "prepare": "pnpm run build",
        "start": "ts-node example.ts",
        "changeset": "changeset",
        "version-packages": "changeset version",
        "release": "pnpm build && changeset publish",
        "postinstall": "pnpm run build"
    },
    "bugs": {
        "url": "https://github.com/binhodev/vyfetch/issues"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.12",
        "@types/node": "^22.13.1",
        "@typescript-eslint/eslint-plugin": "^8.24.0",
        "@typescript-eslint/parser": "^8.24.0",
        "eslint": "^9.20.0",
        "superjson": "^2.2.2",
        "ts-node": "^10.9.2",
        "tsup": "^8.3.6",
        "typescript": "^5.7.3",
        "vitest": "^3.0.5",
        "zod": "^3.24.1"
    },
    "peerDependencies": {
        "superjson": "^2.2.2",
        "zod": "^3.24.1"
    }
}
